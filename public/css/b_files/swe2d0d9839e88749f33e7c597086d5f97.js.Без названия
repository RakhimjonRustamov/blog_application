            setType: function(e, t) {
                this.defaults.type = e;
                this.defaults.name = t
            },
            get: function(elem, opts) {
                var settings = $.extend({}, this.defaults, opts);
                if (!settings.single.length) settings.single = "metadata";
                var data = $.data(elem, settings.single);
                if (data) return data;
                data = "{}";
                var getObject = function(data) {
                    if (typeof data != "string") return data;
                    data = eval("(" + data + ")");
                    return data
                };
                if (settings.type == "html5") {
                    var object = {};
                    $(elem.attributes).each(function() {
                        var e = this.nodeName;
                        if (e.match(/^data-/)) e = e.replace(/^data-/, "");
                        else return true;
                        object[e] = getObject(this.nodeValue)
                    })
                } else {
                    if (settings.type == "class") {
                        var m = settings.cre.exec(elem.className);
                        if (m) data = m[1]
                    } else if (settings.type == "elem") {
                        if (!elem.getElementsByTagName) return;
                        var e = elem.getElementsByTagName(settings.name);
                        if (e.length) data = $.trim(e[0].innerHTML)
                    } else if (elem.getAttribute != undefined) {
                        var attr = elem.getAttribute(settings.name);
                        if (attr) data = attr
                    }
                    object = getObject(data.indexOf("{") < 0 ? "{" + data + "}" : data)
                }
                $.data(elem, settings.single, object);
                return object
            }
        }
    });
    $.fn.metadata = function(e) {
    	//console.log('META', this, arguments);
    	//TODO log some excaption here!
        return $.metadata.get(this[0], e);
    }
    
    // Override data method. this is temporary and should be removed after 
    // replacing all html classes with "data-" attribute.
    var originalDataMethod = $.fn.data;
    $.fn.data = function() {
    	var ret = originalDataMethod.apply( this, arguments );
    	if ((typeof ret == "undefined") && (arguments.length < 2) && (this.length)) {
    		//no "data-" was found
    		var ret2 = $.fn.metadata.apply( this );
    		if (!$.isEmptyObject(ret2)) {
    			if (arguments.length == 1) {
    				return ret2[arguments[0]];
    			}
    			return ret2.metadata;
    		}
    	}
    	return ret;
	}
})(jQuery);;(function($){$.extend($.fn,{validate:function(options){if(!this.length){options&&options.debug&&window.console&&console.warn('Xato: Hech narsa tanlanmagan yoki  tekshirish imkoniyati yoâq');return};var validator=$.data(this[0],'validator');if(validator)return validator;if('noValidate'in document.createElement('form'))this.attr("novalidate","novalidate");validator=new $.validator(options,this[0]);$.data(this[0],'validator',validator);if(validator.settings.onsubmit){var inputsAndButtons=this.find("input, button");inputsAndButtons.filter(".cancel").click(function(){validator.cancelSubmit=true});if(validator.settings.submitHandler)inputsAndButtons.filter(":submit").click(function(){validator.submitButton=this});this.submit(function(event){if(validator.settings.debug)event.preventDefault()
